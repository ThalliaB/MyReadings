@model IEnumerable<MyReadingsModel><div class="container">    <div class="d-flex justify-content-between align-items-center mb-5">        <h1>My Readings</h1>        <a class="btn btn-success mx-2" asp-controller="MyReadings" asp-action="AddNewReading">Add new reading</a>    </div>    <table class="table table-hover">        <thead>            <tr>                <th scope="col">Book</th>                <th scope="col">Writer</th>                <th scope="col">Pages</th>                <th scope="col">Start</th>                <th scope="col">End</th>                <th></th>                @* <th scope="col">Obs</th> *@            </tr>        </thead>        <tbody>            @if(Model != null)            {                            @foreach(var readings in Model)                {                    <tr>                        <th scope="row">@readings.Title</th>                        <td>@readings.Writer</td>                        <td>@readings.Pages</td>                        <td>@readings.Start</td>                        <td>@readings.End</td>                        <td class="dropdown">                            <button class="btn btn-transparent dropdown-toggle btn-sm p-2" type="button" id="options" data-bs-toggle="dropdown" aria-expanded="false"></button>                            <ul class="dropdown-menu">                                <li><a asp-controller="MyReadings" asp-action="OpenViewReading" asp-route-id="@readings.Id" class="dropdown-item">Open</a></li>                                <li><a asp-controller="MyReadings" asp-action="Edit" asp-route-id="@readings.Id" class="dropdown-item">Edit</a></li>                                <li><a asp-controller="MyReadings" asp-action="Delete" asp-route-id="@readings.Id" class="dropdown-item">Delete</a></li>                            </ul>                        </td>                        @* <td>@readings.Obs</td> *@                    </tr>                }            }        </tbody>    </table></div>